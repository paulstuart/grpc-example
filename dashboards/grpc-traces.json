{
  "title": "gRPC Traces",
  "uid": "grpc-traces",
  "version": 3,
  "timezone": "browser",
  "schemaVersion": 39,
  "refresh": "5s",
  "panels": [
    {
      "title": "Recent Traces",
      "type": "table",
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo"
          },
          "queryType": "traceql",
          "query": "{.service.name=\"grpc-example-api\"}",
          "refId": "A",
          "limit": 20
        }
      ],
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "inspect": false
          }
        }
      }
    },
    {
      "title": "Trace Timeline",
      "type": "traces",
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo"
          },
          "queryType": "traceql",
          "query": "{.service.name=\"grpc-example-api\"}",
          "refId": "A",
          "limit": 20
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "tags": ["grpc", "opentelemetry"]
}
